<%= form_for @project do |f| %>

  <%= error_messages_for @project %>
  
  <div class="field">
    <%= f.label :name, "Nome:" %>
    <%= f.text_field :name %>
  </div>
  
  <div class="field">
    <%= f.label :description, "Descrição:" %>
    <%= f.text_field :description %>
  </div>
  
  <div class="field">
    <%= f.label :git, "GitHub:" %>
    <%= f.text_field :git %>
  </div>
  
  <div class="actions">
    <%= f.submit "Criar" %>
  </div>
  
<% end %>
